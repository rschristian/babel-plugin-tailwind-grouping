{
  "name": "babel-plugin-tailwind-grouping",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/plugin.cjs",
      "default": "./dist/plugin.js"
    }
  },
  "license": "MIT",
  "description": "Babel plugin to expand grouped Tailwind utility classes",
  "repository": "https://github.com/rschristian/babel-plugin-tailwind-grouping",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "twind",
    "class",
    "classes",
    "groups",
    "grouping",
    "babel",
    "babel plugin",
    "babel-plugin"
  ],
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "microbundle -f modern,cjs -o dist/plugin.js",
    "test": "node src/test.js",
    "format": "prettier --write --ignore-path .gitignore ."
  },
  "devDependencies": {
    "microbundle": "^0.14.2",
    "prettier": "^2.5.1",
    "prettier-config-rschristian": "^0.1.1",
    "typescript": "^4.5.3",
    "uvu": "^0.5.2",
    "zecorn": "^0.10.1"
  },
  "prettier": "prettier-config-rschristian"
}
