{
  "name": "vite-plugin-tailwind-grouping",
  "version": "0.1.4",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/plugin.cjs",
      "default": "./dist/plugin.js"
    }
  },
  "license": "MIT",
  "description": "Vite plugin to expand grouped Tailwind utility classes",
  "repository": "https://github.com/rschristian/tailwind-grouping",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "twind",
    "class",
    "classes",
    "groups",
    "grouping",
    "vite",
    "vite-plugin",
    "rollup-plugin"
  ],
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "microbundle -f modern,cjs -o dist/plugin.js --target node --no-sourcemap",
    "test": "uvu tests -i fixtures"
  },
  "dependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-syntax-jsx": "^7.16.5",
    "@babel/plugin-syntax-typescript": "^7.16.5"
  },
  "devDependencies": {
    "microbundle": "^0.14.2",
    "uvu": "^0.5.2"
  }
}
